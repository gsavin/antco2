set output "plot-r1.png"
plot "measures-identity.dat" using 1:2 title "identity" with lines, \
"measures-timed.dat" using 1:2 title "timed" with lines, \
"measures-random-timed.dat" using 1:2 title "random timed" with lines, \
"measures-cohesion.dat" using 1:2 title "cohesion" with lines

